{
    "rewrites": [
        {
            "source": "/es/:path*",
            "destination": "/es/index.html"
        },
        {
            "source": "/en/:path*",
            "destination": "/en/index.html"
        }
    ],
    "redirects": [
        {
            "source": "/",
            "has": [
                {
                    "type": "header",
                    "key": "X-Vercel-IP-Country",
                    "value": "DO"
                }
            ],
            "destination": "/es/",
            "permanent": true
        },
        {
            "source": "/:path*",
            "destination": "/en/:path*",
            "permanent": true
        }
    ]
}